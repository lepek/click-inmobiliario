#content-inner {
    overflow: hidden;
}

#gallery {
	width: 500px;
	float:left;	
    border-color: #fff #fff #CCCCCC #fff;
    border-style: solid;
    border-width: 1px;
    box-shadow: 0 0 15px rgba(64, 64, 64, 0.1);
}

#property {
	width: 430px;
    float: right;
    margin: 10px 5px 0px 10px;
}

#property h2 {
    text-align: right;
}

#title {
    width: 100%;
    clear: both;
}