<!DOCTYPE html>
<html>
	<head>
		<title>ClickInmobiliario</title>
		<meta http-equiv="Content-type" content="text/html;charset=UTF-8">
		<%= stylesheet_link_tag :defaults %> <!--[if lte IE 6]>
		<%= stylesheet_link_tag :ie6 %>
		<![endif]-->
		<%= yield :stylesheets %>
		<%= javascript_include_tag :defaults %> <!--[if lte IE 8]><%= javascript_include_tag :ie_html5 %><![endif]-->
		<%= yield :javascripts %>
		<%= csrf_meta_tag %>
		<%= yield :head %>
		<link rel="icon" type="image/png" href="<%= url_for('/images/favicon.png') %>"/>
	</head>
	<body>
		<%= yield %>
	</body>
	<%= yield :scripts %>
</html>
