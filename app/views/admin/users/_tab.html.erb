<fieldset class="gray">
  <%= button_to "Agregar un nuevo usuario", new_admin_user_path, :method => "get" %>
</fieldset>
<% datatable "users-table", ["Nombre", "E-mail", ""], :class => "selectable" do -%>
  <% users.each do |user| -%>
    <tr>
      <td><%= user.first_name %> <%= user.last_name %></td>
      <td><%= user.email %></td>
      <td><%= button_to "Edit", edit_admin_user_path(user), :method => :get %></td>
    </tr>
  <% end -%>
<% end -%>
