<p id="notice"><%= notice %></p>

<p>
  <b>Nombre:</b>
  <%= @user.first_name %>
</p>

<p>
  <b>Apellido:</b>
  <%= @user.last_name %>
</p>

<p>
  <b>E-mail:</b>
  <%= @user.email %>
</p>

<p>
  <b>Rol:</b>
  <%= @user.role.name %>
</p>

<% unless @user.real_estate.nil? %>
<p>
  <b>Inmobiliaria:</b>
  <%= @user.real_estate.name %>
</p>
<% end %>


<%= link_to 'Editar', edit_admin_user_path(@user) %> |
<%= link_to 'Volver', admin_root_path(:anchor => 'users') %>
