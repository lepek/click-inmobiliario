<%= form_for [:admin, @real_estate] do |f| %>
<% if @real_estate.errors.any? %>
<div id="error_explanation">
	<h2><%= t 'errors.header.female', :resource => 'inmobiliaria', :count => @real_estate.errors.count %></h2>
	<ul>
		<% @real_estate.errors.each do |attr, msg| %>
		<li>
			<%= msg %>
		</li>
		<% end %>
	</ul>
</div>
<% end %>
<fieldset class="gray">
	<div class="clearfix">
		<%= f.label :name %>
		<div class="input">
			<%= f.text_field :name %>
		</div>
	</div>
	<div class="clearfix">
		<%= f.label :address %>
		<div class="input">
			<%= f.text_field :address %>
		</div>
	</div>
	<div class="clearfix">
		<%= f.label :email %>
		<div class="input">
			<%= f.text_field :email %>
		</div>
	</div>
	<div class="actions">
		<%= f.submit 'Guardar', :class => "btn primary" %>
	</div>
</fieldset>
<% end %> 