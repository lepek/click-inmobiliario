<fieldset class="gray">
  <%= button_to "Agregar un nuevo inmueble", new_admin_property_path, :method => "get" %>
</fieldset>
<% datatable "properties-table", ["Código", "Dirección", "Inmobiliaria", ""], :class => "selectable" do -%>
  <% properties.each do |property| -%>
    <tr>
      <td><%= property.code %></td>
      <td><%= property.address %></td>
      <td><%= property.code %></td>
      <td><%= button_to "Edit", edit_admin_property_path(property), :method => :get %></td>
    </tr>
  <% end -%>
<% end -%>
